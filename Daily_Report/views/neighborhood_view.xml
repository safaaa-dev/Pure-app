<odoo>
    <data>
        <record id="view_neighborhood_form" model="ir.ui.view">
            <field name="name">ajex.neighborhood.form</field>
            <field name="model">ajex.neighborhood</field>
            <field name="arch" type="xml">
                <form string="Neighborhood Form">
                    <sheet>
                        <group>
                            <field name="name"/>
